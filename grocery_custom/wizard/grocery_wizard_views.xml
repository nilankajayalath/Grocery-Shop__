<odoo>
  <record id="view_grocery_report_wizard_form" model="ir.ui.view">
    <field name="name">grocery.report.wizard.form</field>
    <field name="model">grocery.report.wizard</field>
    <field name="arch" type="xml">
      <form string="Grocery Sales Report Filter">
        <group>
          <field name="start_date"/>
          <field name="end_date"/>
        </group>
        <footer>
          <button name="action_generate_report" type="object" string="Generate Report" class="btn-primary"/>
          <button string="Cancel" special="cancel" class="btn-secondary"/>
        </footer>
      </form>
    </field>
  </record>

 
</odoo>
